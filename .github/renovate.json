{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "labels": ["dependencies"],
  "ignorePaths": ["site/**", "problems/**"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^@tarojs"],
      "enabled": false
    }
  ],
  "ignoreDeps": ["node", "typescript", "eslint-config-taro", "babel-preset-taro"]
}
